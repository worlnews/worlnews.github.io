       \"page_title\": utag_data[\"page_title\"],\n                    \"byline\": utag_data[\"byline\"],\n                    \"pub_date\": [utag_data[\"pub_month\"], utag_data[\"pub_day\"], utag_data[\"pub_year\"]].join('/'),\n                    \"language\": utag_data[\"language\"],\n                    \"language_service\": utag_data[\"language_service\"],\n                    \"short_language_service\":utag_data[\"short_language_service\"],\n                    \"platform\": utag_data[\"platform\"],\n                    \"platform_short\": utag_data[\"platform_short\"],\n                    \"entity\": utag_data[\"entity\"],\n                    \"property_name\": utag_data[\"\"],\n                    \"property_id\": utag_data[\"property_id\"],\n                    \"page_type\": utag_data[\"page_type\"],\n                    \"subcontent_type\": utag_data[\"\"],\n                    \"enviro_type\": utag_data[\"tealium_environment\"],\n                    \"template\": utag_data[\"template\"],\n                    \"slug\": utag_data[\"slug\"],\n                    \"tags\": utag_data[\"tags\"],\n                    \"pub_hour\":utag_data[\"pub_hour\"],\n                           };\n                utag.link(data);\n            };\n</script>\n<script>\n $(window).load(function(){\n              $('.storyaudio .play').attr(\"onclick\",'eventer(\\\".play\\\")');\n            });\n</script>\n\u6700\u8fd1\uff0c\u6e2f\u5e9c\u6b63\u7814\u7a76\u4e0e\u4e2d\u56fd\u5927\u9646\u8bbe\u7acb\u56fa\u5b9a\u7684\u5668\u5b98\u79fb\u690d\u4e92\u52a9\u673a\u5236\uff0c\u5e76\u5f15\u53d1\u793e\u4f1a\u4e89\u8bae\u3002\n\u00a0\n\u7efc\u5408\u65b0\u52a0\u5761\u300a\u8054\u5408\u65e9\u62a5\u300b\u53ca\u9999\u6e2f\u300a\u660e\u62a5\u300b\u62a5\u9053\uff0c\u9999\u6e2f\u533b\u52a1\u536b\u751f\u5c40\u5c40\u957f\u5362\u5ba0\u8302\u4e0a\u5468\u900f\u9732\uff0c\u533b\u9662\u7ba1\u7406\u5c40\u672c\u5468\u5c06\u7387\u9886\u79fb\u690d\u4e13\u5bb6\u524d\u5f80\u5e7f\u4e1c\u7701\u4ea4\u6d41\uff0c\u534f\u52a9\u62df\u5b9a\u4e24\u5730\u5668\u5b98\u4e92\u52a9\u673a\u5236\u5b89\u6392\u3002\u4ed6\u8fd8\u79f0\uff0c\u5927\u9646\u5668\u5b98\u6350\u8d60\u7684\u8d44\u6599\u7ba1\u7406\u7cfb\u7edf\u975e\u5e38\u5148\u8fdb\uff0c\u5b98\u65b9\u5e0c\u671b\u6b64\u884c\u53ef\u52a0\u5f3a\u9999\u6e2f\u5668\u5b98\u79fb\u690d\u754c\u5bf9\u76f8\u5173\u7cfb\u7edf\u7684\u8ba4\u8bc6\u7b49\u3002\n\u00a0\n\u636e\u300a\u8054\u5408\u65e9\u62a5\u300b16\u65e5\u62a5\u9053\u6307\u51fa\uff0c\u9646\u6e2f\u8de8\u5883\u79fb\u690d\u5408\u4f5c\u5728\u9999\u6e2f\u793e\u4f1a\u5f15\u8d77\u4e0d\u5c11\u4e89\u8bae\u3002\u90e8\u5206\u9999\u6e2f\u75c5\u4eba\u548c\u533b\u5b66\u7ec4\u7ec7\u7684\u8d28\u7591\u5305\u62ec\u5927\u9646\u548c\u9999\u6e2f\u5bf9\u201c\u6b7b\u4ea1\u201d\u7684\u5b9a\u4e49\u4e0d\u4e00\uff0c\u4ee5\u53ca\u4e24\u5730\u5bf9\u5668\u5b98\u6350\u8d60\u76d1\u5bdf\u548c\u8fd0\u884c\u673a\u5236\u4e0d\u540c\u7b49\u7b49\u3002\u5404\u65b9\u7684\u6700\u5927\u7591\u8651\uff0c\u5219\u662f\u5927\u9646\u7684\u5668\u5b98\u79fb\u690d\u6765\u6e90\u95ee\u9898\u3002\u8bb8\u591a\u5e74\u524d\uff0c\u4e00\u76f4\u6709\u4f20\u8a00\u6307\u5927\u9646\u5668\u5b98\u6350\u8d60\u6d89\u53ca\u5668\u5b98\u4e70\u5356\u3002\n\u00a0\n\u8de8\u5883\u201c\u5171\u4eab\u201d\u5668\u5b98\u79fb\u690d\u7684\u6d88\u606f\u4e5f\u5f15\u8d77\u4e00\u4e9b\u6e2f\u4eba\u62c5\u5fe7\uff0c\u90e8\u5206\u9999\u6e2f\u6350\u8d60\u8005\u4f1a\u56e0\u4e3a\u4e0d\u613f\u610f\u5c06\u5668\u5b98\u9001\u5f80\u5927\u9646\uff0c\u800c\u9000\u51fa\u6350\u8d60\u8ba1\u5212\uff0c\u6700\u7ec8\u5f71\u54cd\u9999\u6e2f\u75c5\u4eba\u83b7\u5f97\u5668\u5b98\u7684\u673a\u4f1a\u3002\n\u00a0\n\u53bb\u5e7412\u6708, \u9999\u6e2f\u9996\u6b21\u5f15\u5165\u5185\u5730\u5668\u5b98\u6765\u6e2f\uff0c\u8fdb\u884c\u8de8\u5883\u79fb\u690d\u3002\u636e\u5362\u5ba0\u8302\u900f\u9732\uff0c\u4eca\u5e743\u6708\u8bbf\u95ee\u5317\u4eac\u65f6\uff0c\u5df2\u5c31\u4e0e\u5927\u9646\u5c31\u5668\u5b98\u4e92\u52a9\u673a\u5236\u8ba2\u7acb\u521d\u6b65\u65b9\u6848\uff0c4\u6708\uff0c\u4e5f\u66fe\u5230\u5e7f\u5dde\u53c2\u89c2\u4e2d\u56fd\u4eba\u4f53\u5668\u5b98\u5206\u914d\u4e0e\u5171\u4eab\u8ba1\u7b97\u673a\u7cfb\u7edf\uff08COTRS\uff09\u3002\n\u00a0\n\u591a\u5e74\u6765\uff0c\u6709\u5173\u4e2d\u56fd\u5927\u9646\u5f3a\u6458\u6cd5\u8f6e\u529f\u7b49\u826f\u5fc3\u72af\u548c\u7ef4\u543e\u5c14\u4eba\u5668\u5b98\u7684\u62a5\u9053\u4e0d\u65ad\uff0c\u56fd\u9645\u4eba\u6743\u673a\u6784\u4e00\u76f4\u8d28\u7591\u4e2d\u56fd\u6bcf\u5e74\u6350\u732e\u5668\u5b98\u6570\u91cf\u7a00\u5c11\u4e0e\u5e9e\u5927\u7684\u5b9e\u9645\u79fb\u690d\u5668\u5b98\u6570\u91cf\u4e4b\u95f4\u7684\u5de8\u5927\u5dee\u5f02\u3002\n\u00a0\n\u8d23\u7f16\uff1a\u51ef\u8fea", "images": ["https://www.rfa.org/mandarin/Xinwen/1-05162023110138.html/@@images/42d4c454-7df7-4893-bb4b-ed795021ce4c.jpeg"], "imagesn": ["000000.png"]}