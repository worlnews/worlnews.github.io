ce\"],\n                    \"platform\": utag_data[\"platform\"],\n                    \"platform_short\": utag_data[\"platform_short\"],\n                    \"entity\": utag_data[\"entity\"],\n                    \"property_name\": utag_data[\"\"],\n                    \"property_id\": utag_data[\"property_id\"],\n                    \"page_type\": utag_data[\"page_type\"],\n                    \"subcontent_type\": utag_data[\"\"],\n                    \"enviro_type\": utag_data[\"tealium_environment\"],\n                    \"template\": utag_data[\"template\"],\n                    \"slug\": utag_data[\"slug\"],\n                    \"tags\": utag_data[\"tags\"],\n                    \"pub_hour\":utag_data[\"pub_hour\"],\n                           };\n                utag.link(data);\n            };\n</script>\n<script>\n $(window).load(function(){\n              $('.storyaudio .play').attr(\"onclick\",'eventer(\\\".play\\\")');\n            });\n</script>\n<p>\n \u53f0\u6e7e\u5916\u4ea4\u90e89\u53f7\u516c\u5e03\uff0c\u82f1\u56fd\u524d\u9996\u76f8\u7279\u62c9\u65af\uff08Liz Truss MP\uff09\u5c06\u4e8e5\u670816\u65e5\u81f320\u65e5\u8bbf\u95ee\u53f0\u6e7e\uff0c\u9664\u4e86\u5c06\u664b\u89c1\u5e9c\u9662\u9ad8\u5c42\u3001\u62dc\u4f1a\u76f8\u5173\u90e8\u4f1a\u9996\u957f\uff0c\u4e5f\u5c06\u53c2\u8bbf\u53f0\u6e7e\u6587\u7ecf\u8bbe\u65bd\u3002\u5916\u4ea4\u90e8\u6307\u51fa\uff0c\u7279\u62c9\u65af\u524d\u9996\u76f8\u5e76\u5e94\u8fdc\u666f\u57fa\u91d1\u4f1a\u9080\u8bf7\u4e8e5\u670817\u65e5\u53d1\u8868\u6f14\u8bb2\uff0c\u4e5f\u5c06\u4e0e\u53f0\u6e7e\u653f\u3001\u5546\u3001\u5b66\u7b49\u5404\u754c\u4eba\u58eb\u4ea4\u6d41\u4e92\u52a8\uff0c\u5f3a\u5316\u53f0\u82f1\u5bc6\u5207\u53cb\u597d\u5173\u7cfb\u3002\n</p>\n<p>\n \u53f0\u6e7e\u5916\u4ea4\u90e8\u6307\u51fa\uff0c\u7279\u62c9\u65af\u524d\u9996\u76f8\u4e3a\u53f0\u6e7e\u957f\u671f\u575a\u5b9a\u53cb\u4eba\uff0c\u82f1\u56fd\u653f\u5e9c\u5728\u5979\u62c5\u4efb\u5916\u76f8\u671f\u95f4\uff0c\u591a\u6b21\u4e2a\u522b\u6216\u4e0e\u5176\u4ed6G7\u56fd\u5bb6\u8868\u8fbe\u7ef4\u6301\u53f0\u6d77\u548c\u5e73\u7a33\u5b9a\u7684\u91cd\u8981\u6027\uff0c\u5e76\u8868\u793a\u82f1\u56fd\u5728\u786e\u4fdd\u53f0\u6d77\u548c\u5e73\u7a33\u5b9a\u4e0a\u5177\u6709\u660e\u786e\u5229\u76ca\u3002\u5979\u4e8e2022\u5e744\u6708\u53d1\u8868\u82f1\u56fd\u5916\u4ea4\u653f\u7b56\u6f14\u8bf4\u4e2d\uff0c\u9996\u5ea6\u63d0\u53ca\u5e94\u786e\u4fdd\u53f0\u6e7e\u7b49\u6c11\u4e3b\u653f\u4f53\u5177\u5907\u81ea\u6211\u9632\u536b\u80fd\u529b\uff0c\u5e76\u5728\u53bb\u5e746\u6708\u5317\u7ea6\u5cf0\u4f1a\u671f\u95f4\u8868\u793a\uff0c\u53f0\u6e7e\u7684\u5b89\u5168\u4e0d\u4ec5\u6538\u5173\u519b\u4e8b\u5b89\u5168\uff0c\u4e5f\u5173\u4e4e\u7ecf\u6d4e\u5b89\u5168\uff0c\u591a\u6b21\u53f7\u53ec\u6c11\u4e3b\u9635\u8425\u652f\u6301\u53f0\u6e7e\uff0c\u5c06\u53f0\u6d77\u548c\u5e73\u7a33\u5b9a\u91cd\u8981\u6027\u4ece\u533a\u57df\u63d0\u5347\u81f3\u5168\u7403\u5c42\u6b21\u3002\n <br/>\n <br/>\n \u8d23\u7f16 \u8bb8\u4e66\u5a77\n</p>", "images": ["https://www.rfa.org/mandarin/Xinwen/st-05092023050236.html/@@images/7d02a623-0337-40d0-95b7-561c99fba976.jpeg"], "imagesn": ["000000.png"]}