utag_data[\"language\"],\n                    \"language_service\": utag_data[\"language_service\"],\n                    \"short_language_service\":utag_data[\"short_language_service\"],\n                    \"platform\": utag_data[\"platform\"],\n                    \"platform_short\": utag_data[\"platform_short\"],\n                    \"entity\": utag_data[\"entity\"],\n                    \"property_name\": utag_data[\"\"],\n                    \"property_id\": utag_data[\"property_id\"],\n                    \"page_type\": utag_data[\"page_type\"],\n                    \"subcontent_type\": utag_data[\"\"],\n                    \"enviro_type\": utag_data[\"tealium_environment\"],\n                    \"template\": utag_data[\"template\"],\n                    \"slug\": utag_data[\"slug\"],\n                    \"tags\": utag_data[\"tags\"],\n                    \"pub_hour\":utag_data[\"pub_hour\"],\n                           };\n                utag.link(data);\n            };\n</script>\n<script>\n $(window).load(function(){\n              $('.storyaudio .play').attr(\"onclick\",'eventer(\\\".play\\\")');\n            });\n</script>\n<p class=\"p1\">\n \u6839\u636e\u97e9\u8054\u793e\u62a5\u5bfc\uff0c\u636e\u97e9\u56fd\u9a7b\u534e\u5927\u4f7f\u9986\u65b9\u9762\u6d88\u606f\uff0c\u6548\u529b\u4e8e\u5c71\u4e1c\u6cf0\u5c71\u8db3\u7403\u4ff1\u4e50\u90e8\u7684\u97e9\u56fd\u5916\u63f4\u5b59\u51c6\u6d6912\u65e5\u88ab\u8fbd\u5b81\u7701\u8b66\u65b9\u5e26\u8d70\u8c03\u67e5\uff0c\u9886\u533a\u9886\u4e8b\u6b63\u5bf9\u5b59\u51c6\u6d69\u63d0\u4f9b\u5fc5\u8981\u7684\u9886\u4e8b\u534f\u52a9\u3002\n</p>\n<p class=\"p1\">\n 5\u670816\u65e5\uff0c\u4e2d\u56fd\u5916\u4ea4\u90e8\u53d1\u8a00\u4eba\u6c6a\u6587\u658c\u79f0\uff0c\u636e\u4e86\u89e3\uff0c\u8fd1\u65e5\u6709\u4e00\u540d\u97e9\u56fd\u516c\u6c11\u56e0\u6d89\u5acc\u975e\u56fd\u5bb6\u5de5\u4f5c\u4eba\u5458\u53d7\u8d3f\u7f6a\uff0c\u88ab\u8fbd\u5b81\u7701\u516c\u5b89\u673a\u5173\u4f9d\u6cd5\u5211\u4e8b\u62d8\u7559\u3002\u8fbd\u5b81\u7701\u516c\u5b89\u673a\u5173\u5df2\u5411\u97e9\u56fd\u9a7b\u6c88\u9633\u603b\u9886\u4e8b\u9986\u8fdb\u884c\u4e86\u9886\u4e8b\u901a\u62a5\uff0c\u5e76\u5c06\u4e3a\u97e9\u65b9\u9886\u4e8b\u5b98\u5458\u6267\u884c\u9886\u4e8b\u804c\u52a1\u63d0\u4f9b\u5fc5\u8981\u4fbf\u5229\u3002\n</p>\n<p class=\"p1\">\n \u97e9\u8054\u793e\u62a5\u5bfc\uff0c\u5b59\u51c6\u6d69\u4ece12\u65e5\u8d77\u5904\u4e8e\u88ab\u201c\u5211\u4e8b\u62d8\u7559\u201d\u72b6\u6001\uff0c\u5728\u8fbd\u5b81\u7701\u63a5\u53d7\u8c03\u67e5\u3002\u4e2d\u56fd\u516c\u5b8915\u65e5\u5411\u97e9\u56fd\u9a7b\u6c88\u9633\u603b\u9886\u4e8b\u9986\u901a\u544a\u4e86\u5b59\u51c6\u6d69\u88ab\u5211\u62d8\u7684\u60c5\u51b5\u3002\n</p>\n<p class=\"p1\">\n \u636e\u4e2d\u56fd\u5a92\u4f53\u62a5\u9053\uff0c\u5b59\u51c6\u6d69\u88ab\u8b66\u65b9\u8c03\u67e5\u6216\u4e0e\u5c71\u4e1c\u6cf0\u5c71\u4e3b\u5e05\u90dd\u4f1f\u7275\u6d89\u7684\u201c\u5047\u7403\u6848\u201d\u6709\u5173\u3002\u5728\u672c\u8f6e\u8db3\u575b\u6253\u5047\u98ce\u66b4\u4e2d\uff0c\u91d1\u656c\u9053\u7b49\u5c71\u4e1c\u6cf0\u5c71\u9009\u624b\u6d89\u6848\u88ab\u8c03\u67e5\u3002\n</p>\n<p class=\"p1\">\n \u5b59\u51c6\u6d69\u51fa\u751f\u4e8e1992\u5e74\uff0c2014\u5e74\u8fdb\u5165\u6d66\u9879\u5236\u94c1\u8db3\u7403\u5177\u4e50\u90e8\u5f00\u542f\u804c\u4e1a\u751f\u6daf\uff0c2014\u5e74\u53c2\u52a0\u4ec1\u5ddd\u4e9a\u8fd0\u4f1a\uff0c\u52a9\u529b\u7403\u961f\u6458\u5f97\u91d1\u724c\uff0c2018\u5e74\u8f6c\u5165\u5168\u5317\u73b0\u4ee3\u6c7d\u8f66\u5177\u4e50\u90e8\uff0c2021\u5e74\u8f6c\u5165\u5c71\u4e1c\u6cf0\u5c71\u3002\u5b59\u51c6\u6d692020\u83b7\u9009K1\u8054\u8d5b\u6700\u4f73\u7403\u5458\uff08MVP\uff09\uff0c\u53bb\u5e74\u8fd8\u4ee3\u8868\u97e9\u56fd\u51fa\u6218\u5361\u5854\u5c14\u4e16\u754c\u676f\u3002\n</p>\n<p class=\"p1\">\n \u8d23\u7f16 \u8bb8\u4e66\u5a77\n</p>", "images": ["https://www.rfa.org/mandarin/Xinwen/st2-05162023044712.html/@@images/e6b067d9-534b-4598-9f81-49249f0a5386.jpeg"], "imagesn": ["000000.png"]}