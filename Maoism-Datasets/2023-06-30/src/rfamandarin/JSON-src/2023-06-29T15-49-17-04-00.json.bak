ntity\": utag_data[\"entity\"],\n                    \"property_name\": utag_data[\"\"],\n                    \"property_id\": utag_data[\"property_id\"],\n                    \"page_type\": utag_data[\"page_type\"],\n                    \"subcontent_type\": utag_data[\"\"],\n                    \"enviro_type\": utag_data[\"tealium_environment\"],\n                    \"template\": utag_data[\"template\"],\n                    \"slug\": utag_data[\"slug\"],\n                    \"tags\": utag_data[\"tags\"],\n                    \"pub_hour\":utag_data[\"pub_hour\"],\n                           };\n                utag.link(data);\n            };\n</script>\n<script>\n $(window).load(function(){\n              $('.storyaudio .play').attr(\"onclick\",'eventer(\\\".play\\\")');\n            });\n</script>\n\u6e56\u5357\u7941\u4e1c\u53bf\u516c\u6c11\u8c2d\u5175\u6797\u6d89\u5acc\u5bfb\u8845\u6ecb\u4e8b\u7f6a\u4e00\u6848\u5df2\u7ecf\u5f00\u5ead\uff0c\u4f46\u5ba1\u5224\u7ed3\u679c\u8fd8\u672a\u516c\u5e03\uff0c\u6848\u4ef6\u5ba1\u7406\u6b63\u8f6c\u79fb\u5230\u522b\u7684\u5730\u65b9\uff0c\u5bb6\u5c5e\u76ee\u524d\u4e5f\u4e0d\u6e05\u695a\u3002\n\u00a0\n\u636e\u6c11\u751f\u89c2\u5bdf\u7f51\u7684\u6d88\u606f\uff0c2022\u5e746\u6708\uff0c\u8c2d\u5175\u6797\u5230\u6751\u59d4\u4f1a\u4e3e\u724c\u6297\u8bae\u4ed6\u7236\u6bcd\u88ab\u5f3a\u884c\u63a5\u79cd\u65b0\u51a0\u75ab\u82d7\u4e00\u4e8b\uff0c\u7acb\u5373\u906d\u5230\u7941\u4e1c\u53bf\u8b66\u65b9\u6293\u6355\uff0c\u81ea\u6b64\u540e\u5931\u8054\u3002\n\u00a0\n\u8fc7\u4e86\u6570\u5929\uff0c\u8c2d\u7236\u83b7\u77e5\uff0c\u8c2d\u5175\u6797\u88ab\u6d89\u5acc\u5bfb\u8845\u6ecb\u4e8b\u7f6a\u62d8\u6355\u3002\n\u00a0\n\u5176\u540e\u6570\u4e2a\u6708\uff0c\u8c2d\u5175\u6797\u7684\u5f8b\u5e08\u591a\u6b21\u8981\u6c42\u4f1a\u89c1\u5f53\u4e8b\u4eba\uff0c\u636e\u906d\u62d2\u7edd\u3002\n\u00a0\n\u636e\u4e86\u89e3\uff0c\u8c2d\u5175\u6797\u66fe\u53c2\u4e0e\u63a8\u52a8\u6559\u80b2\u5e73\u6743\u8fd0\u52a8\uff0c\u5021\u5bfc\u514d\u8d39\u533b\u7597\u3001