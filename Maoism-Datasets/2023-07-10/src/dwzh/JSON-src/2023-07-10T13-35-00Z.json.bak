u7684\u96c7\u4f63\u519b\u58eb\u5175\u524d\u5f80\u767d\u4fc4\u7f57\u65af\u3002\u4e0d\u786e\u5b9a\u6709\u591a\u5c11\u4eba\u4f1a\u8ddf\u4ed6\u8d70\u3002 \u4e0d\u8fc7\u74e6\u683c\u7eb3\u519b\u56e2\u7684\u751f\u610f\u904d\u53ca\u4e16\u754c\u5404\u5730\uff0c\u5229\u6bd4\u4e9a\u3001\u53d9\u5229\u4e9a\u3001\u975e\u6d32\u591a\u4e2a\u56fd\u5bb6\u4ee5\u53ca\u62c9\u7f8e\u7684\u59d4\u5185\u745e\u62c9\u90fd\u662f\u8fd9\u652f\u96c7\u4f63\u519b\u635e\u91d1\u7684\u5730\u65b9\u3002\u4e0a\u6218\u573a\u7684\u58eb\u5175\u636e\u79f0\u6bcf\u6708\u53ef\u4ee5\u62ff\u52305000\u7f8e\u5143\u519b\u9977\u3002\u666e\u91cc\u6208\u6d25\u81ea\u79f0\u519b\u961f\u4ecd\u67092.5\u4e07\u4eba\uff0c\u7f8e\u56fd\u60c5\u62a5\u5355\u4f4d\u4f30\u8ba1\u8fd9\u4e2a\u6570\u5b57\u5e94\u8be5\u7ffb\u500d\u3002\" height=\"394\" itemprop=\"image\" loading=\"lazy\" src=\"https://static.dw.com/image/66024976_303.webp\" title=\"\u7406\u8bba\u4e0a\u4ed6\u53ef\u4ee5\u5e26\u8d70\u4ed6\u7684\u96c7\u4f63\u519b\u58eb\u5175\u524d\u5f80\u767d\u4fc4\u7f57\u65af\u3002\u4e0d\u786e\u5b9a\u6709\u591a\u5c11\u4eba\u4f1a\u8ddf\u4ed6\u8d70\u3002 \u4e0d\u8fc7\u74e6\u683c\u7eb3\u519b\u56e2\u7684\u751f\u610f\u904d\u53ca\u4e16\u754c\u5404\u5730\uff0c\u5229\u6bd4\u4e9a\u3001\u53d9\u5229\u4e9a\u3001\u975e\u6d32\u591a\u4e2a\u56fd\u5bb6\u4ee5\u53ca\u62c9\u7f8e\u7684\u59d4\u5185\u745e\u62c9\u90fd\u662f\u8fd9\u652f\u96c7\u4f63\u519b\u635e\u91d1\u7684\u5730\u65b9\u3002\u4e0a\u6218\u573a\u7684\u58eb\u5175\u636e\u79f0\u6bcf\u6708\u53ef\u4ee5\u62ff\u52305000\u7f8e\u5143\u519b\u9977\u3002\u666e\u91cc\u6208\u6d25\u81ea\u79f0\u519b\u961f\u4ecd\u67092.5\u4e07\u4eba\uff0c\u7f8e\u56fd\u60c5\u62a5\u5355\u4f4d\u4f30\u8ba1\u8fd9\u4e2a\u6570\u5b57\u5e94\u8be5\u7ffb\u500d\u3002\" width=\"700\"/>\n  <h4>\n   \u74e6\u683c\u7eb3\u4f63\u519b24\u5c0f\u65f6\u7684\u77ed\u547d\u66b4\u52a8\n  </h4>\n  <h2>\n   \u591a\u5c11\u4eba\u4f1a\u8ddf\u4ed6\u8d70\uff1f\n  </h2>\n  <p>\n   \u7406\u8bba\u4e0a\u4ed6\u53ef\u4ee5\u5e26\u8d70\u4ed6\u7684\u96c7\u4f63\u519b\u58eb\u5175\u524d\u5f80\u767d\u4fc4\u7f57\u65af\u3002\u4e0d\u786e\u5b9a\u6709\u591a\u5c11\u4eba\u4f1a\u8ddf\u4ed6\u8d70\u3002 \u4e0d\u8fc7\u74e6\u683c\u7eb3\u519b\u56e2\u7684\u751f\u610f\u904d\u53ca\u4e16\u754c\u5404\u5730\uff0c\u5229\u6bd4\u4e9a\u3001\u53d9\u5229\u4e9a\u3001\u975e\u6d32\u591a\u4e2a\u56fd\u5bb6\u4ee5\u53ca\u62c9\u7f8e\u7684\u59d4\u5185\u745e\u62c9\u90fd\u662f\u8fd9\u652f\u96c7\u4f63\u519b\u635e\u91d1\u7684\u5730\u65b9\u3002\u4e0a\u6218\u573a\u7684\u58eb\u5175\u636e\u79f0\u6bcf\u6708\u53ef\u4ee5\u62ff\u52305000\u7f8e\u5143\u519b\u9977\u3002\u666e\u91cc\u6208\u6d25\u81ea\u79f0\u519b\u961f\u4ecd\u67092.5\u4e07\u4eba\uff0c\u7f8e\u56fd\u60c5\u62a5\u5355\u4f4d\u4f30\u8ba1\u8fd9\u4e2a\u6570\u5b57\u5e94\u8be5\u7ffb\u500d\u3002\n  </p>\n </li>\n</ul>\n<br class=\"clearfix\"/>\n<a href=\"javascript:void(0);\">\n</a>\n<a href=\"javascript:void(0);\">\n</a>\n<a href=\"javascript:void(0);\">\n</a>\n<a href=\"javascript:void(0);\">\n</a>\n<a href=\"javascript:void(0);\">\n</a>\n<span class=\"slideshowNav\">\n</span>\n<p>\n \u5728\u666e\u91cc\u6208\u6d25\u9886\u5bfc\u7684\u77ed\u6682\u5175\u53d8\u671f\u95f4\uff0c\u74e6\u683c\u7eb3\u6b66\u88c5\u5206\u5b50\u63a7\u5236\u4e86\u5357\u90e8\u57ce\u5e02\u987f\u6cb3\u7554\u7f57\u65af\u6258\u592b\u548c\u4e00\u4e2a\u519b\u4e8b\u5927\u697c\uff0c\n <a href=\"https://www.dw.com/zh/zh/\u74e6\u683c\u7eb3\u70e7\u4fc4\u53a8\u5b50\u64a4\u9000\u7136\u540e\u5462/a-66025628\">\n  \u4f46\u8be5\u5175\u53d8\u5728\u767d\u4fc4\u7f57\u65af\u9886\u5bfc\u4eba\u5362\u5361\u7533\u79d1\u65a1\u65cb\u4e0b\u8fc5\u901f\u7ec8\u6b62\u3002\n </a>\n</p>\n<p>\n <strong>\n  \u66f4\u591a\u9605\u8bfb\uff1a\n  <a href=\"https://www.dw.com/zh/zh/\u5ba2\u5ea7\u8bc4\u8bba\u666e\u91cc\u6208\u6d25\u7684\u5175\u53d8\u4f55\u4ee5\u672a\u80fd\u6210\u4e8b/a-66052574\">\n   \u666e\u91cc\u6208\u6d25\u7684\u5175\u53d8\u4f55\u4ee5\u672a\u80fd\u6210\u4e8b\n  </a>\n </strong>\n</p>\n<p>\n \u666e\u4eac\u5f53\u65f6\u5c06\u8fd9\u6b21\u4e8b\u4ef6\u6bd4\u4f5c1917\u5e74\u4fc4\u7f57\u65af\u9769\u547d\u524d\u5915\u5e2d\u5377\u4fc4\u7f57\u65af\u7684\u52a8\u4e71\uff0c\u540e\u6765\u4ed6\u611f\u8c22\u4ed6\u7684\u519b\u961f\u548c\u5b89\u5168\u90e8\u95e8\u5236\u6b62\u6df7\u4e71\u548c\u5185\u6218\u3002\n</p>\n<p>\n \u666e\u91cc\u6208\u6d25\u66fe\u8868\u793a\uff0c\u5175\u53d8\u7684\u76ee\u7684\u4e0d\u662f\u63a8\u7ffb\u653f\u5e9c\uff0c\u800c\u662f\u4e3a\u4e86\u5c06\u519b\u961f\u9ad8\u5c42\u9886\u5bfc \u201c\u7ef3\u4e4b\u4ee5\u6cd5\u201d\uff0c\u6307\u63a7\u4ed6\u4eec\u5728\u4e4c\u514b\u5170\u72af\u4e0b\u4e86\u9519\u8bef\u5e76\u8fdb\u884c\u4e86\u4e0d\u4e13\u4e1a\u7684\u884c\u52a8\u3002\n</p>\n<p>\n \u4ece\u4e24\u4eba\u5728\u56fd\u5bb6\u7535\u89c6\u53f0\u7684\u9732\u9762\u6765\u770b\uff0c\u666e\u4eac\u8fc4\u4eca\u4e3a\u6b62\u4fdd\u7559\u4e86\u56fd\u9632\u90e8\u957f\u7ecd\u4f0a\u53e4\u548c\u603b\u53c2\u8c0b\u957f\u683c\u62c9\u897f\u83ab\u592b\u7684\u804c\u4f4d\uff0c\u62d2\u7edd\u4e86\u666e\u91cc\u6208\u6d25\u89e3\u96c7\u4ed6\u4eec\u7684\u8981\u6c42\u3002\n</p>\n<p>\n \u6839\u636e\u767d\u4fc4\u7f57\u65af\u9886\u5bfc\u4eba\u5362\u5361\u7533\u79d1\u65a1\u65cb\u8fbe\u6210\u7684\u534f\u8bae\uff0c\u666e\u91cc\u6208\u6d25\u672c\u5e94\u524d\u5f80\u767d\u4fc4\u7f57\u65af\u3002\n <a href=\"https://www.dw.com/zh/zh/\u767d\u4fc4\u7e3d\u7d71\u74e6\u683c\u7d0d\u9996\u8166\u8eab\u8655\u4fc4\u7f85\u65af/a-66147076\">\n  \u4f46\u5362\u5361\u7533\u79d1\u4e0a\u5468\u8868\u793a\uff0c\u666e\u91cc\u6208\u6d25\u5df2\u8fd4\u56de\u4fc4\u7f57\u65af\n </a>\n \uff0c\u74e6\u683c\u7eb3\u5c06\u58eb\u4e5f\u6ca1\u6709\u8f6c\u79fb\u5230\u767d\u4fc4\u7f57\u65af\uff0c\u8fd9\u5f15\u53d1\u4e86\u4eba\u4eec\u5bf9\u534f\u8bae\u6267\u884c\u60c5\u51b5\u7684\u6000\u7591\u3002\n</p>\n<p>\n \u5175\u53d8\u6d41\u4ea7\u5df2\u7ecf\u8fc7\u4e86\u4e24\u4e2a\u661f\u671f\uff0c\u7ec8\u6b62\u5175\u53d8\u7684\u534f\u8bae\u4f9d\u65e7\u79d8\u800c\u4e0d\u5ba3\uff0c\u74e6\u683c\u7eb3\u96c6\u56e2\u7684\u547d\u8fd0\u4ecd\u7136\u60ac\u800c\u672a\u51b3\u3002\n</p>\n<p>\n <strong>\n  \u66f4\u591a\u9605\u8bfb\uff1a\n  <a href=\"https://www.dw.com/zh/zh/\u4fc4\u7f57\u65af\u793e\u4f1a\u5982\u4f55\u770b\u5f85\u74e6\u683c\u7eb3\u53db\u53d8/a-66053987\">\n   \u4fc4\u7f57\u65af\u793e\u4f1a\u5982\u4f55\u770b\u5f85\u74e6\u683c\u7eb3\u53db\u53d8\uff1f\n  </a>\n </strong>\n</p>\n<p>\n \uff08\u8def\u900f\u793e\uff0c\u6cd5\u65b0\u793e\uff09\n</p>", "images": []}